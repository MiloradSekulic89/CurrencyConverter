<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="VboxEchangeRateWindow" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.currencyexchangerfinal.SceneControllerCurrency">

    <children>
        <MenuBar fx:id="MenuBarExchangeRateWindow" prefHeight="29.0" prefWidth="602.0">
            <menus>
                <Menu fx:id="MenuFileExchangeRateWindow" mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem fx:id="MenuItemCloseExchangeRateWindow" mnemonicParsing="false" onAction="#close" text="Close" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <TextArea fx:id="TextAreaExchangeRateWIndow" minHeight="-Infinity" minWidth="-Infinity" prefHeight="343.0" prefWidth="600.0" />
        <Button fx:id="ShowButton" mnemonicParsing="false" onAction="#CurrencyList" text="Show currency list">
            <VBox.margin>
                <Insets bottom="20.0" left="250.0" top="50.0" />
            </VBox.margin>
        </Button>
        <Button fx:id="BackToHelloView" mnemonicParsing="false" onAction="#switchToSceneHello" text="Go back">
            <VBox.margin>
                <Insets left="280.0" />
            </VBox.margin>
        </Button>
        <HBox fx:id="HboxMenu" prefHeight="25.0" prefWidth="600.0" />
        <VBox fx:id="VBoxExchangeRateWindow" prefHeight="378.0" prefWidth="600.0" />
    </children>
</VBox>
