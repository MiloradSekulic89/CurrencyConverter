            <Insets top="200.0" />
         </VBox.margin>
      </Text>
      <Text fx:id="aboutText2" strokeType="OUTSIDE" strokeWidth="0.0" text="Created by Commi Software solutions" wrappingWidth="278.13671875" />
      <Text fx:id="aboutText3" strokeType="OUTSIDE" strokeWidth="0.0" text="Contact email : commisoftwaresolutions@gmail.com" wrappingWidth="278.13671875" />
      <Button fx:id="Back" mnemonicParsing="false" onAction="#switchToSceneHello" prefHeight="39.0" prefWidth="122.0" text="Back">
